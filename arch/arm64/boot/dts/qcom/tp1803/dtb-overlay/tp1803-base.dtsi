#include "tp1803-msm-audio-lpass.dtsi"
#include "tp1803-sm8150-audio.dtsi"
#include "tp1803-sm8150-pcie.dtsi"
#include "tp1803-sm8150-pinctrl.dtsi"

&shared_meta {
	parts = "vbmeta,boot,system,vendor,dtbo";
};

&android_q_fstab {
	/delete-node/ odm;
	vendor {
		fsmgr_flags = "wait";
		status = "ok";
	};
};

&reserved_memory {
	ramoops: ramoops@0xa9800000 {
		compatible = "ramoops";
		reg = <0 0xa9800000 0 0x00400000>;
		record-size = <0x40000>;
		console-size = <0x40000>;
		ftrace-size =  <0x40000>;
		pmsg-size= <0x200000>;
		devinfo-size= <0x01000>;
		ecc-size= <0x0>;
	};
};
